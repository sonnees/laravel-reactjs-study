=== Mock ===
$userService = $this->createMock(UserService::class);

$userService->expects($this->once())
->method('func_1')
->willReturn(true);

->willReturn(collect([
['id' => 1, 'name' => 'Product 1'],
['id' => 2, 'name' => 'Product 2']
]));