=== SQL ===
Query Builder
$users = DB::table('users')
->where('status', 'active')
->orderBy('created_at', 'desc')
->get();

Eloquent
$users = User::where('status', 'active')
->orderBy('created_at', 'desc')
->get();

SQL raw
$users = DB::select('SELECT * FROM users WHERE status = ?', ['active']);

